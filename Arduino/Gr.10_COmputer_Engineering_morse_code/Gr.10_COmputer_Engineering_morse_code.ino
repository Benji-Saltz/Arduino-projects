int ledPin1=2;

void setup(){
  pinMode(ledPin1,OUTPUT);

}
void loop(){
  digitalWrite(ledPin1,HIGH);
    delay(100);
      digitalWrite(ledPin1,LOW); 
      delay(100);
      digitalWrite(ledPin1,HIGH);
     delay(100);
       digitalWrite(ledPin1,LOW); 
       delay(100);
       digitalWrite(ledPin1,HIGH);
      delay(100);
        digitalWrite(ledPin1,LOW); 
        delay(100);
        digitalWrite(ledPin1,HIGH);
      delay(500);
        digitalWrite(ledPin1,LOW);
        delay(100); 
        digitalWrite(ledPin1,HIGH);
      delay(500);
        digitalWrite(ledPin1,LOW);
        delay(100); 
        digitalWrite(ledPin1,HIGH);
      delay(500);
        digitalWrite(ledPin1,LOW);
        delay(100); 
        digitalWrite(ledPin1,HIGH);
      delay(100);
        digitalWrite(ledPin1,LOW);
        delay(100); 
        digitalWrite(ledPin1,HIGH);
     delay(100);
       digitalWrite(ledPin1,LOW);
       delay(100); 
       digitalWrite(ledPin1,HIGH);
      delay(100);
    digitalWrite(ledPin1,LOW); 
      delay(1000);
 
 
}
